{
  "expo": {
    "name": "dfUEL MakerSpace",
    "slug": "dfuel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/dfuel-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/dfuel-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#7c3aed"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/dfuel-logo.png",
        "backgroundColor": "#7c3aed"
      }
    },
    "web": {
      "favicon": "./public/dfuel-logo.png"
    },
    "plugins": [
      "expo-dev-client"
    ]
  }
}
